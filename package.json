{
  "name": "@edx/frontend-app-library-authoring",
  "version": "0.1.0",
  "description": "Library authoring application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-library-authoring.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-library-authoring#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-app-library-authoring/issues"
  },
  "dependencies": {
    "@edx/frontend-component-footer": "10.0.10",
    "@edx/frontend-platform": "1.3.7",
    "@edx/paragon": "10.0.1",
    "@fortawesome/fontawesome-svg-core": "1.2.29",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@reduxjs/toolkit": "^1.2.3",
    "babel-polyfill": "6.26.0",
    "edx-pattern-library": "0.18.1",
    "immutability-helper": "3.0.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-dropzone": "^11.0.2",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@edx/frontend-build": "3.0.0",
    "codecov": "3.7.1",
    "copy-webpack-plugin": "^6.0.3",
    "es-check": "5.1.0",
    "glob": "7.1.6",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "reactifex": "1.1.1",
    "rosie": "2.0.1",
    "webpack-merge": "4.2.2"
  }
}
